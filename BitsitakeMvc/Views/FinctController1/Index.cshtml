
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />


    <script src="C:\Users\PC\Source\Repos\BitsitakeMvc\BitsitakeMvc\Scripts\jquery-3.4.1.js"></script>
    <title>Index</title>
</head>
<body>
    <div>
        <input id="btnName" type="text" name="Name" />
        <input id="btnAge" type="text" name="Age" />
        <input id="btnAdd" type="submit" value="submit" />
    </div>
</body>
</html>
<script>
    $('#btnAdd').click(function () {
        var data = {
            Name: $('#btnName').val(),
            Age: $('#Age').val(),
        }
        $.ajax({
            url: 'http://localhost:52142/api/Defunct/MeName',
            type: 'get',
            data: data,
            dataType:'json',
            success: function (d) {
                if (d > 0) {
                    alert('成功');
                }
                else {
                    alert('失败');
                }
            }
        })
    })
</script>
